<%- include('../partials/head.ejs') %>
  <h1>All Flights</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(f => { %>
        <tr>
          <td><%= f.airline %> </td>
          <td><%= f.airport %> </td>
          <td><%= f.flightNo %> </td>
          <td><%= f.departs %> </td>
        </tr>
      <% }) %>
    </tbody>
    <br>
  </table>
<%- include('../partials/footer.ejs') %> 
